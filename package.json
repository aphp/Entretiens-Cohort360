{
  "name": "cohort360-exercises",
  "version": "1.0.0",
  "description": "Exercices techniques Fullstack & Data - AP-HP Cohort360 Monorepo",
  "private": true,
  "packageManager": "npm@10.8.0",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev --parallel",
    "lint": "turbo lint",
    "test": "turbo test",
    "typecheck": "turbo typecheck",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "test:api": "playwright test e2e/api/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aphp/Entretiens-Cohort360.git"
  },
  "keywords": ["cohort360", "healthcare", "django", "react", "typescript", "playwright"],
  "author": "Guillaume FORTAINE",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aphp/Entretiens-Cohort360/issues"
  },
  "homepage": "https://github.com/aphp/Entretiens-Cohort360#readme",
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "turbo": "^2.5.4"
  }
}
